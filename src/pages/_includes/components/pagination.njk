<div class="pagination">
  {% if pagination.href.previous %}
    <a href="{{ pagination.href.previous | url }}">Previous</a>
  {% else %}
    <span>Previous</span>
  {% endif %}

  {% if pagination.href.next %}
    <a href="{{ pagination.href.next | url }}">Next</a>
  {% else %}
    <span>Next</span>
  {% endif %}
</div>
